<p id="notice"></p>
<div class="container">
 <div class="row">
 
  <%= render 'books/list', user: @users,book:@book %>
 
   <div class="col-md-8">
    <h2>Users</h2>
     <table class="table">
      <thead>
       <tr>
        <td>image</td>
        <td>name</td>
        <td colspan="3"></td>
       </tr>
      </thead>
     <tbody>
      <% @user.each do |user| %>
       <tr>
        <td><%= image_tag user.get_profile_image(100,100) %></td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path(user.id) %></td>
       </tr>
       <% end %>
     </tbody>
    </table>
   </div>
 </div>
</div>